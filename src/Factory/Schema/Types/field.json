{
  "key": "field",
  "label": "Custom Field",
  "documentation": "https://www.advancedcustomfields.com/resources/register-fields-via-php/",
  "groups": [
    {
      "label": "Attributes",
      "expandable": true,
      "schema": {
        "key": {
          "label": "Key",
          "description": "(string) Unique identifier for field group.",
          "type": "id",
          "prefix": "group_",
          "required": true,
          "placeholder": "my-field-group"
        },
        "label": {
          "label": "Label",
          "description": "(string) Visible in metabox handle",
          "type": "text",
          "required": true,
          "placeholder": "My Field Group"
        },
        "type": {
          "label": "Type",
          "description": "(string) Type of field",
          "type": "select",
          "options": [
            "text",
            "image",
            "checkbox"
          ]
        }
      }
    }
  ],
  "table": {
    "columns": [
      {
        "label": "Key/ID",
        "key": "key",
        "type": "key"
      },
      {
        "label": "Label",
        "key": "label",
        "type": "string"
      },
      {
        "label": "Type",
        "key": "type",
        "type": "string"
      }
    ]
  }
}
